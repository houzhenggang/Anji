    <!-- 创建摄像头设备列表数据库 -->
    <string name="sql_create_camere_list_tbl">	    CREATE TABLE tb_device_list(
	    _id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
	    devType NUMERIC NULL,
	    devName VARCHAR(64) NULL,
	    ip VARCHAR(64) NULL,
	    streamType NUMERIC DEFAULT 0 NULL,
	    webPort NUMERIC NULL,
	    mediaPort NUMERIC NULL,
	    uid VARCHAR(128) NULL,
	    userName VARCHAR(64) NULL,
	    password VARCHAR(64) NULL,
	    devSetName VARCHAR(64) NULL,
	    groupName VARCHAR(64) NULL,
	    groupId NUMERIC NULL,
	    thumPath VARCHAR(64) NULL,
	    cameraId INTEGER NULL
		)</string>
    <!-- 创建开关，传感设备列表数据库 -->
    <string name="sql_create_device_tbl">	    CREATE TABLE tb_anji_device(
	    _id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
	    memberId VARCHAR(64) NULL,
	    ssuid VARCHAR(64) NULL,
	    deviceMac VARCHAR(64) NULL,
	    deviceId INTEGER NULL,
	    deviceChannel INTEGER NULL,
	    deviceChannel2 INTEGER NULL,
	    deviceType VARCHAR(64) NULL,
	    type INTEGER NULL,
	    deviceState INTEGER NULL,
	    sensorState INTEGER NULL,
	    deviceName VARCHAR(64) NULL,
	    groupName VARCHAR(64) NULL,
	    groupID INTEGER NULL,
	    deviceBattery INTEGER NULL,
	    isCharge INTEGER NULL,
	    tempValue NUMERIC NULL,
	    humValue NUMERIC NULL,
	    infraredSwitch INTEGER NULL
		)</string>
    <string name="sql_create_group_tbl">	    CREATE TABLE tb_anji_group(
	    _id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
	    groupID INTEGER NULL,
	    groupName VARCHAR(64) NULL,
	    memberId VARCHAR(64) NULL,
	    ssuid VARCHAR(64) NULL,
	    iconType VARCHAR(64) NULL
		)</string>
    <string name="drop_sql">drop table if EXISTS tb_device_list;</string>
    <string name="drop_sql_device">drop table if EXISTS tb_anji_device;</string>
    <string name="drop_sql_group">drop table if EXISTS tb_anji_group;</string>
